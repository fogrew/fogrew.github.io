---
permalink: /manifest.json
eleventyExcludeFromCollections: true
---
{
  "name": "{{ metadata.title }}",
  "description": "{{ metadata.description }}",
  "lang": "ru",
  "start_url": "{{ '/' | url }}",
  "display": "minimal-ui",
  "theme_color": "#f7f6ee",
  "background_color": "#f7f6ee",
  "icons": [
      {
          "src": "{{ 'img/favicon/android-chrome-192x192.png' | url }}",
          "sizes": "192x192",
          "type": "image/png"
      },
      {
          "src": "{{ '/img/favicon/android-chrome-512x512.png' | url }}",
          "sizes": "512x512",
          "type": "image/png"
      }
  ]
}
